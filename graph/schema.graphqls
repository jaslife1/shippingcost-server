input Address {
  street: String
  city: String
  province: String
  zipcode: Int
}

type Query {
  allProvinces: [String]!
  allCitiesOfProvince(province:String):[String]!
  calculateShippingCost(senderAddress: Address!, receiverAddress: Address!) : Int!
}



